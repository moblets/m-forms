{
  "fields": [
    {
      "name": "title",
      "type": "input",
      "inputType": "text",
      "maxlength": 20,
      "validations": [{ "name": "maxLength", "value": 20 }]
    },
    {
      "name": "description",
      "type": "textarea",
      "matAutosizeMinRows": 5,
      "matAutosizeMaxRows": 10,
      "maxlength": 255,
      "validations": [{ "name": "maxLength", "value": 255 }]
    },
    {
      "name": "cover",
      "type": "imageUploader",
      "api": "m-forms",
      "fileConfig": [
        { "title": "maxSize", "value": "5MB" },
        { "title": "fileFormat", "value": "PNG, JPG, JPEG" }
      ],
      "formats": ["image/png", "image/jpg", "image/jpeg", "image/gif"],
      "maxSize": 5000000
    },
    {
      "name": "fields",
      "type": "formFields"
    }
  ]
}
